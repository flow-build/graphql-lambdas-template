    "lint:fix": "turbo run lint:fix"
  },
  "devDependencies": {
    "@serverless/typescript": "3.29.0",
    "@types/aws-lambda": "8.10.114",
    "@types/node": "18.15.11",
    "dotenv-cli": "7.2.1",
    "eslint-config-custom": "*",
    "ts-node": "10.9.1",
    "turbo": "latest",
    "typescript": "5.0.3"
  },
  "dependencies": {
    "aws-lambda": "1.0.7"
  }
}
